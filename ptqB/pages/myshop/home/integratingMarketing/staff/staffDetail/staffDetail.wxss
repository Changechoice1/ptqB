@import '/pages/myshop/home/integratingMarketing/staff/staff';
.rank_divide {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 30rpx;
}

.divide_1,
.divide_2 {
    display: flex;
    align-items: center;
    width: 50%;
}

.divide_1 {
    justify-content: flex-start;
}

.divide_2 {
    justify-content: flex-end;
}

.divide_img {
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;
}

.divide_icon {
    width: 32rpx;
    height: 32rpx;
    margin-left: 10rpx;
}

.info {
    background: #fff;
    padding-bottom: 30rpx;
}

.info_head {
    padding: 30rpx;
    border-bottom: 2rpx solid #eee;
    display: flex;
    align-items: center;
}

.info_img {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.info_name_1 {
    color: #666;
}

.info_name_2 {
    margin-top: 10rpx;
}

.info_name_2>text {
    color: #999;
}

.info_main {
    padding: 30rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.info_m_block {
    width: 50%;
    text-align: center;
}

.info_m_t {
    color: #666;
}

.info_m_money {
    font-size: 1.2em;
    color: #e60012;
    margin-top: 20rpx;
}

.info_m_num {
    margin-top: 10rpx;
}

.divide {
    color: #666;
    padding: 30rpx;
}

.list_block {
    background: #fff;
    padding: 30rpx;
    border-bottom: 2rpx solid #eee;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.detail_list .list_block:last-child {
    border-bottom: none;
}

.list_t {
    color: #999;
    margin-top: 10rpx;
}

.noStaff {
    width: 100%;
    height: 100rpx;
    background: #fff;

}