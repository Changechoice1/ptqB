/*物品抖动事件*/

.bottomtx {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  color: #999;
  text-align: center;
  font-size: 24rpx;
  background: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}

.bottomtx image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
  animation: loadingIco 0.5s linear infinite;
}

@keyframes loadingIco {
  from {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(360deg);
  }
}

.twoFontNow {
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.onFontNow {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

