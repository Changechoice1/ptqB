  height: 100%;
  line-height: 80rpx;
  font-size: 28rpx;
  border-radius: 10rpx;
  /* box-shadow: 0 4rpx 8rpx #aaa; */
}

button {
  position: relative;
  display: block;
  margin: 0;
  padding: 0;
  box-sizing: box;
  font-size: 28rpx;
  text-align: center;
  text-decoration: none;
  line-height: 100rpx;
  border-radius: 0px;
  -webkit-tap-highlight-color: transparent;
  overflow: hidden;
  color: #000;
  background-color: transparent;
}

button::after {
  content: " ";
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  border: 0px solid rgba(0, 0, 0, 0.2);
  -webkit-transform: scale(0);
  transform: scale(0);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  box-sizing: border-box;
  border-radius: 0px;
}

.tabBarBox {
  width: 100%;
  height: 120rpx;
  position: fixed;
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-top: 1rpx #b5b5b5 solid;
  background: #fff;
  z-index: 999;
  bottom: 0rpx;
}

.tabBarBox .tabBarBox_in_box {
  width: 80rpx;
  height: 100rpx;
  display: flex;
  justify-content: center;
  -ms-align-items: center;
  align-items: center;
  flex-wrap: wrap;
}

.tabBarBox .tabBarBox_in_box image {
