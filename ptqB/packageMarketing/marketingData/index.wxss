	background-color:#fff;
} */

.saveImgBtn {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 60rpx;
}

.saveImgBtn_in {
  height: 92rpx;
  width: 366rpx;
  background-color: #e60012;
  line-height: 92rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
  font-weight: bold;
  border-radius: 20rpx;
}
