/* packageMarketing/industryCustom/index.wxss */
@import '/template/template.wxss';
page{
  background-color:#fff;
}
.index_box{
  width:100%;
  background-color:#fff;
}
.top-title{
  height:88rpx;
  line-height:88rpx;
  display: flex;
  justify-content: space-between;
   padding:0 30rpx;
   background-color:#f5f5f5;
}
.top-l{
  font-size:32rpx;
  color:#333;
  font-weight: 700;
}
.top-r{
  font-size:24rpx;
  color:#999;
}

.type_tab{
  display: flex;
  padding: 0 30rpx;
  border-bottom: 2rpx solid #e8e8e8;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 28rpx;
  font-weight: 500;
  text-align: center;
}
.type_tab_item{
  width: 33%;
}
.type_tab_item_on{
  border-bottom: 2rpx solid #e60012;
  color: #e60012;
}

.custom-list{
  padding: 30rpx;
  display: flex;
  display:-webkit-flex;
  flex-warp:wrap;
  -webkit-flex-wrap:wrap;
  justify-content: space-between;
}
/* 行业定制为空 */
.null_data{
  width:100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin:200rpx 0;
}
.null_img{
  width:260rpx;
  height:260rpx;
}
.no_solu{
  font-size:32rpx;
  color:#333;
  margin:40rpx 0;
}


