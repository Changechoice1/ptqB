/* 悬浮换行业图片 start*/

.changeIndus {
  position: fixed;
  bottom: 200rpx;
  right: 20rpx;
  z-index: 9;
  width: 180rpx;
  height: 120rpx;
}

.shake {
  animation: shake 1s;
  -o-animation: shake 1s;
  -webkit-animation: shake 1s;
  -moz-animation: shake 1s;
}

@keyframes shake {
  0%, 100% {
    transform: rotate(10deg);
  }

  10%, 30%, 50%, 70%, 90% {
    transform: rotate(-10deg);
  }

  20%, 40%, 60%, 80% {
    transform: rotate(10deg);
  }
}

